{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/store",
  "builds": [
    {
      "src": "dist/store/server/server.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server/server.mjs"
    }
  ],
  "functions": {
    "dist/store/server/server.mjs": {
      "maxDuration": 30
    }
  }
}
